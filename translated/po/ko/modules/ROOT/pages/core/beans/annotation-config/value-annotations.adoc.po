msgid ""
"\t\tpublic MovieRecommender(@Value(\"#{systemProperties['user.catalog'] + "
"'Catalog' }\") String catalog) {\n"
"\t\t\tthis.catalog = catalog;\n"
"\t\t}\n"
"\t}\n"
msgstr ""

#. type: delimited block -
