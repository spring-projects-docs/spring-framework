#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/web/webmvc/mvc-controller/ann-modelattrib-methods.adoc:38
#, no-wrap
msgid ""
"\t@ModelAttribute\n"
"\tpublic void populateModel(@RequestParam String number, Model model) {\n"
"\t\tmodel.addAttribute(accountRepository.findAccount(number));\n"
"\t\t// add more ...\n"
"\t}\n"
msgstr ""

#. type: Labeled list
