#, no-wrap
msgid ""
"\t\t@GetMapping(version = \"1.1\") // <2>\n"
"\t\tpublic Account getAccount1_1() {\n"
"\t\t}\n"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/web/webflux/controller/ann-requestmapping.adoc:427
#, no-wrap
msgid ""
"\t\t@GetMapping(version = \"1.2+\") // <3>\n"
"\t\tpublic Account getAccount1_2() {\n"
"\t\t}\n"
msgstr ""

