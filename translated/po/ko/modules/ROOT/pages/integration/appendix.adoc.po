#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:60
#, no-wrap
msgid "\t<jee:jndi-lookup id=\"dataSource\" jndi-name=\"jdbc/MyDataSource\"/>\n"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:65
#, no-wrap
msgid ""
"\t<bean id=\"userDao\" class=\"com.foo.JdbcUserDao\">\n"
"\t\t<!-- Spring will do the cast automatically (as usual) -->\n"
"\t\t<property name=\"dataSource\" ref=\"dataSource\"/>\n"
"\t</bean>\n"
msgstr ""

#. type: Title ====
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:70
#, no-wrap
msgid "`<jee:jndi-lookup/>` (with Single JNDI Environment Setting)"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:74
msgid ""
"The following example shows how to use JNDI to look up an environment "
"variable without `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:85
#, no-wrap
msgid ""
"\t<bean id=\"simple\" "
"class=\"org.springframework.jndi.JndiObjectFactoryBean\">\n"
"\t\t<property name=\"jndiName\" value=\"jdbc/MyDataSource\"/>\n"
"\t\t<property name=\"jndiEnvironment\">\n"
"\t\t\t<props>\n"
"\t\t\t\t<prop key=\"ping\">pong</prop>\n"
"\t\t\t</props>\n"
"\t\t</property>\n"
"\t</bean>\n"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:88
msgid ""
"The following example shows how to use JNDI to look up an environment "
"variable with `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:94
#, no-wrap
msgid ""
"\t<jee:jndi-lookup id=\"simple\" jndi-name=\"jdbc/MyDataSource\">\n"
"\t\t<jee:environment>ping=pong</jee:environment>\n"
"\t</jee:jndi-lookup>\n"
msgstr ""

#. type: Title ====
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:98
#, no-wrap
msgid "`<jee:jndi-lookup/>` (with Multiple JNDI Environment Settings)"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:102
msgid ""
"The following example shows how to use JNDI to look up multiple environment "
"variables without `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:114
#, no-wrap
msgid ""
"\t<bean id=\"simple\" "
"class=\"org.springframework.jndi.JndiObjectFactoryBean\">\n"
"\t\t<property name=\"jndiName\" value=\"jdbc/MyDataSource\"/>\n"
"\t\t<property name=\"jndiEnvironment\">\n"
"\t\t\t<props>\n"
"\t\t\t\t<prop key=\"sing\">song</prop>\n"
"\t\t\t\t<prop key=\"ping\">pong</prop>\n"
"\t\t\t</props>\n"
"\t\t</property>\n"
"\t</bean>\n"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:118
msgid ""
"The following example shows how to use JNDI to look up multiple environment "
"variables with `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:128
#, no-wrap
msgid ""
"\t<jee:jndi-lookup id=\"simple\" jndi-name=\"jdbc/MyDataSource\">\n"
"\t\t<!-- newline-separated, key-value pairs for the environment (standard "
"Properties format) -->\n"
"\t\t<jee:environment>\n"
"\t\t\tsing=song\n"
"\t\t\tping=pong\n"
"\t\t</jee:environment>\n"
"\t</jee:jndi-lookup>\n"
msgstr ""

#. type: Title ====
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:132
#, no-wrap
msgid "`<jee:jndi-lookup/>` (Complex)"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:136
msgid ""
"The following example shows how to use JNDI to look up a data source and a "
"number of different properties without `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:147
#, no-wrap
msgid ""
"\t<bean id=\"simple\" "
"class=\"org.springframework.jndi.JndiObjectFactoryBean\">\n"
"\t\t<property name=\"jndiName\" value=\"jdbc/MyDataSource\"/>\n"
"\t\t<property name=\"cache\" value=\"true\"/>\n"
"\t\t<property name=\"resourceRef\" value=\"true\"/>\n"
"\t\t<property name=\"lookupOnStartup\" value=\"false\"/>\n"
"\t\t<property name=\"expectedType\" value=\"com.myapp.DefaultThing\"/>\n"
"\t\t<property name=\"proxyInterface\" value=\"com.myapp.Thing\"/>\n"
"\t</bean>\n"
msgstr ""

#. type: Plain text
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:151
msgid ""
"The following example shows how to use JNDI to look up a data source and a "
"number of different properties with `jee`:"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/integration/appendix.adoc:161
#, no-wrap
msgid ""
"\t<jee:jndi-lookup id=\"simple\"\n"
"\t\t\tjndi-name=\"jdbc/MyDataSource\"\n"
"\t\t\tcache=\"true\"\n"
"\t\t\tresource-ref=\"true\"\n"
"\t\t\tlookup-on-startup=\"false\"\n"
"\t\t\texpected-type=\"com.myapp.DefaultThing\"\n"
"\t\t\tproxy-interface=\"com.myapp.Thing\"/>\n"
msgstr ""

