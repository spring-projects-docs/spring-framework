#, no-wrap
msgid "Java"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/testing/testcontext-framework/ctx-management/dynamic-property-sources.adoc:77
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/testing/testcontext-framework/ctx-management/dynamic-property-sources.adoc:100
#, no-wrap
msgid ""
"\t@SpringJUnitConfig(/* ... */)\n"
"\t@Testcontainers\n"
"\tclass ExampleIntegrationTests {\n"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/testing/testcontext-framework/ctx-management/dynamic-property-sources.adoc:81
#, no-wrap
msgid ""
"\t\t@Container\n"
"\t\tstatic GenericContainer redis =\n"
"\t\t\tnew GenericContainer(\"redis:5.0.3-alpine\").withExposedPorts(6379);\n"
msgstr ""

#. type: delimited block -
#: /mnt/c/Users/etrees/Desktop/private/spring-framework/origin/framework-docs/modules/ROOT/pages/testing/testcontext-framework/ctx-management/dynamic-property-sources.adoc:87
#, no-wrap
msgid ""
"\t\t@DynamicPropertySource\n"
"\t\tstatic void redisProperties(DynamicPropertyRegistry registry) {\n"
"\t\t\tregistry.add(\"redis.host\", redis::getHost);\n"
"\t\t\tregistry.add(\"redis.port\", redis::getFirstMappedPort);\n"
"\t\t}\n"
msgstr ""

