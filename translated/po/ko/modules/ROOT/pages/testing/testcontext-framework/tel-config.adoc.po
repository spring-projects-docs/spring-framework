#, no-wrap
msgid ""
"\t// Switch to default listeners\n"
"\t@TestExecutionListeners(\n"
"\t\tlisteners = [],\n"
"\t\tinheritListeners = false,\n"
"\t\tmergeMode = MERGE_WITH_DEFAULTS)\n"
